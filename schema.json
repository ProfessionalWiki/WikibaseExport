{
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"entityLabelLanguage": {
			"type": [ "string", "null" ]
		},
		"chooseSubjectsLabel": {
			"type": [ "string", "null" ]
		},
		"filterSubjectsLabel": {
			"type": [ "string", "null" ]
		},
		"defaultSubjects": {
			"type": "array",
			"items": {
				"type": "string",
				"pattern": "^[qQ][1-9]\\d{0,9}$"
			}
		},
		"defaultStartYear": {
			"type": [ "integer", "null" ]
		},
		"defaultEndYear": {
			"type": [ "integer", "null" ]
		},
		"startTimePropertyId": {
			"$ref": "#/$defs/property"
		},
		"endTimePropertyId": {
			"$ref": "#/$defs/property"
		},
		"pointInTimePropertyId": {
			"$ref": "#/$defs/property"
		},
		"properties": {
			"type": "array",
			"items": {
				"$ref": "#/$defs/property"
			}
		},
		"introText": {
			"type": [ "string", "null" ]
		}
	},
	"dependentSchemas": {
		"defaultStartYear": {
			"properties": {
				"defaultEndYear": {
					"minimum": {
						"$data": "1/defaultStartYear"
					}
				}
			}
		}
	},
	"$defs": {
		"property": {
			"type": [ "string", "null" ],
			"pattern": "^[pP][1-9]\\d{0,9}$"
		}
	}
